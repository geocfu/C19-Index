{
  "expo": {
    "name": "C19-Index",
    "slug": "C19-Index",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/medical-mask.png",
    "scheme": "myapp",
    "splash": {
      "image": "./src/assets/images/medical-mask.png",
      "resizeMode": "native",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    }
  }
}
