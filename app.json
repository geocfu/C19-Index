{
  "expo": {
    "name": "C19-Index",
    "slug": "C19-Index",
    "platforms": ["ios", "android", "web"],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./src/assets/images/medical-mask.png",
    "scheme": "c19index",
    "splash": {
      "image": "./src/assets/images/medical-mask.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "description": "A mobile app (iOS & Android) build with Expo for displaying the vulnerability of a patient on the COVID-19 pandemic.\n",
    "githubUrl": "https://github.com/geocfu/C19-Index"
  }
}
